import acm.graphics.*;
import acm.program.*;
import java.awt.*;

public class ProgramHierarchy extends GraphicsProgram {
	
	
	public void run() {
		int posx = getWidth()/2;
		int posy = getHeight()/2;
		GLabel label = new GLabel("Program",posx,posy);
		add(label);
	}
}

