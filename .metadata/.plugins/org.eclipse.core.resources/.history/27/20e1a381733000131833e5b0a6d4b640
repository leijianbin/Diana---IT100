import acm.graphics.*;
import acm.program.*;
import java.awt.*;

public class ProgramHierarchy extends GraphicsProgram {
	
	
	public void run() {
		int centerx = getWidth()/2;
		int centery = getHeight()/2;
		
		int posx = centerx - (label.getWidth()/2);
		int posy = centery - label.getAscent()/2;
		
		GLabel label = new GLabel("Program",posx,posy);
		add(label);
	}
}

